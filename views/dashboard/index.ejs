  <script>
    function editItem(id) {
      $.get('/dashboard/edit-item-screen?id=' + id, (e) => $('.content').html(e))
      $('#screen_name').html('עריכת מוצר')
    }
  </script>
  <script>
    $('.navbar-toggler').click((e) => {
      $('#dash-sidebar').toggleClass('sidebar-open')
    })
    if ($('#dash-sidebar').hasClass('sidebar-open')) {
      $('body').click(() => {
        $('#dash-sidebar').removeClass('sidebar-open')
      })
    }

    $('.sidebar-wrapper li').click(() => {
      $('#dash-sidebar').removeClass('sidebar-open')
    })

    var picker = new CP(document.getElementById('color_picker'));
    function onChangePicker(r, g, b, a) {
      this.source.value = this.color(r, g, b, a);
      // this.source.backgroundColor = 'rgab(r, g, b, a)';
    }
    picker.on('change', onChangePicker);
  </script>

